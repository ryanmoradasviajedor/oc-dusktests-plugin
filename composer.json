{
    "name": "pikanji/dusktests-plugin",
    "type": "october-plugin",
    "description": "Enables to write test using Laravel's Dusk test framework.",
    "license": "MIT",
    "authors": [
        {
            "name": "Kanji Furuhashi"
        }
    ],
    "require": {
        "php": ">=7.0",
        "composer/installers": "~1.0",
        "october/system": ">=1.0.420",
        "laravel/dusk": "^2.0"
    },
    "autoload": {
        "classmap": [
            "traits/CreatesApplication.php"
        ],
        "psr-4": {
            "Tests\\": "../../../tests/"
        }
    }
}
